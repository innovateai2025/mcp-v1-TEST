# Configuracion del Servidor MCP La Cabrera
# Copiar este archivo como .env y completar con los valores reales

# Configuracion de N8N
N8N_API_URL=http://your-n8n-instance:5678/api/v1
N8N_API_KEY=your-n8n-api-key

# Configuracion de Google Sheets
GOOGLE_SHEETS_ID=your-google-sheets-id
GOOGLE_SHEETS_CREDENTIALS=/app/credentials.json

# Configuracion de Base de Datos (opcional)
DATABASE_URL=postgresql://user:password@localhost:5432/lacabrera

# Configuracion de Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
ADMIN_EMAIL=admin@lacabrera.com

# Configuracion de Logging
LOG_LEVEL=INFO
